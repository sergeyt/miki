doctype html
html(lang="en")
	include header
	body
		include nav
		.row(style="padding: 8px")
			// left panel - wiki list
			.col-md-4
				.panel.panel-default
					.panel-heading
						p.panel-title="Wikis"
					.panel-body
						ul.list-group
							each wiki in wikis
								li.list-group-item
									a(href=wiki.href)=wiki.name
			// right panel - wiki preview
			.col-md-8
				.panel.panel-default
					.panel-heading
						p.panel-title="Preview"
					.panel-body
